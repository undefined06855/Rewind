{
    "geode": "4.3.1",
    "gd": {
        "win": "2.2074",
        "mac": "2.2074",
        "android": "2.2074",
        "ios": "2.2074"
    },

    "version": "v1.0.7",
    "id": "undefined0.rewind",
    "name": "Rewind",
    "developer": "undefined0",
    "description": "It's Rewind time - quickly go back to a previous point in the level!",
    "tags": [ "gameplay" ],

    "resources": {
        "sprites": [ "resources/*.png" ]
    },

    "dependencies": {
        "geode.custom-keybinds": {
            "version": "v1.10.3",
            "importance": "required",
            "platforms": [
                "win",
                "android32",
                "android64",
                "macos"
            ]
        },
        "geode.node-ids": "v1.20.1"
    },

    "links": {
        "source": "https://github.com/undefined06855/Rewind"
    },

    "settings": {
        "frames-per-second": {
            "name": "Frames per second",
            "description": "The amount of captured frames every second.",
            "type": "int",
            "min": 2,
            "max": 60,
            "default": {
                "android": 15,
                "ios": 15,
                "win": 20,
                "mac": 20
            },
            "control": { "arrows": false, "slider-step": 5 }
        },
        "history-length": {
            "name": "History length",
            "description": "The length of rewind history stored, in capture frame count. This will be attempted to be automatically populated on windows machines with Nvidia or AMD graphics cards by the amount of video memory available.",
            "type": "int",
            "min": 20,
            "max": 500,
            "default": {
                "android": 140,
                "ios": 140,
                "win": 180,
                "mac": 180
            },
            "control": { "arrows": false, "slider-step": 5 }
        },
        "frame-resolution": {
            "name": "Frame resolution multiplier",
            "description": "A multiplier for the resolution every frame is captured at.",
            "type": "float",
            "min": 0.2,
            "max": 1.5,
            "default": {
                "android": 0.8,
                "ios": 0.8,
                "win": 0.9,
                "mac": 1.0
            },
            "control": { "arrows": false }
        }
    }
}
